{
  "name": "accesa-ui-starter",
  "version": "0.1.8",
  "description": "starter package for web-based projects",
  "main": "index.js",
  "scripts": {
    "start": "webpack-dev-server --open --mode development",
    "build": "webpack -p",
    "css:build": "node-sass -r sass sass/main.scss -o css --trace",
    "css:build-compressed": "node-sass -r --output-style compressed sass sass/main.scss -o css",
    "css:build-minified": "uglifycss css/main.css > css/main.min.css",
    "css:build-nested": "node-sass -r --output-style nested sass sass/main.scss -o css",
    "css:watch": "npm run css:build && npm run css:build -- -w",
    "css:lint": "stylelint css/main.css > css/lintOutput.txt",
    "sass:lint": "stylelint sass/**/**/**/*.scss > sass/lintOutput.txt"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/alexandrumacra/accesa-ui-starter"
  },
  "author": "alex macra",
  "license": "OSL-3.0",
  "devDependencies": {
    "font-awesome": "^4.7.0",
    "stylelint": "^9.1.1",
    "stylelint-config-sass-guidelines": "^5.0.0",
    "stylelint-scss": "^2.5.0",
    "uglifycss": "0.0.28",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.4",
    "babel-preset-es2015": "^6.24.1",
    "css-loader": "^0.28.11",
    "node-sass": "^4.8.3",
    "sass-loader": "^6.0.7",
    "style-loader": "^0.20.3",
    "webpack": "^4.5.0",
    "webpack-cli": "^2.0.14",
    "webpack-dev-server": "^3.1.1"
  },
  "dependencies": {
    "sass-globbing": "^1.0.0-alpha1"
  }
}
